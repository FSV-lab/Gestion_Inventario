<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/resourses/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>

        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Administrar_cliente</from-outcome>
            <to-view-id>/cliente/List.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Detalle_Venta</from-outcome>
            <to-view-id>/detalleVenta/List.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Administrar_Factura</from-outcome>
            <to-view-id>/factura/List.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Administrar_Producto</from-outcome>
            <to-view-id>/producto/List.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Administrar_Proveedor</from-outcome>
            <to-view-id>/proveedor/List.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Administrar_usuario</from-outcome>
            <to-view-id>/usuario/List.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>
</faces-config>
